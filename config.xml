<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.wisc.wid.fielddaylab.siftr" version="3.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Siftr</name>
  <description>
    Explore and analyze your world through photos.
  </description>
  <author email="djgagnon@wisc.edu" href="http://wid.wisc.edu/programs/fielddaylab/">
    Field Day Lab
  </author>
  <icon src="icon.png" />
  <content src="index.html" />
  <access origin="*" />
  <access launch-external="yes" origin="mailto:*" />
  <access launch-external="yes" origin="tel:*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <preference name="StatusBarOverlaysWebView" value="false" />
  <preference name="StatusBarBackgroundColor" value="#000000" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <plugin name="cordova-plugin-statusbar" spec="~2.1.1" />
</widget>
